{
  "name": "quicksend-extension",
  "displayName": "Quicksend extension",
  "version": "0.0.1",
  "description": "Quicksend Extension",
  "author": "aleksey_pr",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "lucide-react": "^0.344.0",
    "plasmo": "^0.90.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactjs-popup": "^2.0.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@types/chrome": "0.1.27",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "prettier": "3.6.2",
    "typescript": "5.9.3"
  },

  "manifest": {
    "name": "Qucksend",
    "version": "1.0",
    "description": "QuickSend - это современный сервис для массовой отправки электронных писем внутри Gmail. Мы гарантируем простое в использовании приложение, которое оптимизирует вашу рассылку, минимизируя число писем в спаме.",
    "host_permissions": [
      "*://mail.google.com/*",
      "http://localhost:8000"
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "cookies",
      "alarms",
      "https://www.googleapis.com/*"
    ]
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
